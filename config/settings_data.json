      "mission-header-cta": {
        "type": "mission-header-cta",
        "settings": {
          "header_text": "<p><strong>We&#x27;re An Alkaline<br\/>Water<\/strong> In A Bottle That<br\/>You Can Feel Good About!<\/p>",
          "text_content": "<p>Our unique bottle is made from 100% recycled, certified ocean-bound plastic that was rescued from at-risk coastal environments, preventing future pollution in the world’s oceans. <br\/><\/p>",
          "btn_text": "Shop Now",
          "btn_url": "shopify:\/\/collections\/all-products",
          "image": "shopify:\/\/shop_images\/Asset_4_3x-80_d4f9b842-a197-4a6b-9d3d-c4723e256a6a.jpg",
