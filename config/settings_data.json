        "settings": {
          "title": "<p>Please Recycle<br\/><strong>ZenWTR<\/strong><\/p>",
          "text": "Help us keep the loop closed. Learn more\nabout how easy it is to recycle ZenWTR.",
          "light_text": false,
          "cta_text": "Dive Deeper",
